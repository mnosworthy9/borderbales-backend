{
  "type": "pwa-node",
  "request": "launch",
  "name": "Debug Dev Env",
  "runtimeArgs": [
      "-r",
      "ts-node/register",
      "-r",
      "tsconfig-paths/register",
  ],
  "args": [
      "${workspaceFolder:express-gen-ts}/src/index.ts"
  ],
  "resolveSourceMapLocations": [
      "${workspaceFolder}/**",
      "!**/node_modules/**"
],
"configurations": [
    {
        "command": "npm start",
        "name": "Run npm start",
        "request": "launch",
        "type": "node-terminal"
    },
    

    {
        "type": "pwa-chrome",
        "name": "http://localhost:4000",
        "request": "launch",
        "url": "http://localhost:4000"
    }
],
}